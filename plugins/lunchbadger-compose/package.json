{
  "name": "lunchbadger-compose",
  "private": true,
  "version": "0.0.1",
  "description": "Compose plugin for LunchBadger",
  "main": "",
  "scripts": {
    "preinstall": "rimraf node_modules && node ./bin/preinstall",
    "clean": "rimraf dist/*",
    "dist": "npm run clean && webpack --env=dist --progress",
    "dist:local": "npm run clean && webpack --env=dev --progress",
    "lint": "eslint ./src",
    "release:major": "npm version major && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "test": "npm run lint",
    "start": "webpack --env=dev --watch --progress"
  },
  "repository": "",
  "keywords": [],
  "author": "Your name here",
  "peerDependencies": {
    "jsplumb": "^2.0.7",
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^15.0.1"
  }
}
